{
  "name": "goodvibespreserver",
  "version": "1.0.0",
  "description": "Browser extension that blocks unpleasant content",
  "scripts": {
    "build": "find ./scripts \\( -path \"./scripts/tools\" \\) -prune -o -iname \"*.ts\" -exec sh -c 'bun build \"{}\" --outfile \"${0%.*}\".js' {} ';'",
    "dev": "bun run build && web-ext run --devtools",
    "dev-update": "bun x npm-check-updates -u"
  },
  "keywords": [],
  "author": "Cofeiini <cofeiini@gmail.com> (https://github.com/Cofeiini)",
  "maintainers": [
    "Franco Gargiulo <francogargiulo2004@gmail.com> (https://github.com/kralloie)"
  ],
  "license": "ISC",
  "dependencies": {
    "spark-md5": "^3.0.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^1.5.3",
    "@types/firefox": "^0.0.34",
    "@types/firefox-webext-browser": "^120.0.0",
    "@types/spark-md5": "^3.0.4",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "ts-loader": "^9.5.1",
    "web-ext": "^7.9.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
