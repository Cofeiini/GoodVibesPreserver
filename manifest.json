{
  "manifest_version": 2,
  "name": "Good Vibes Preserver",
  "version": "1.0",
  "description": "Extension meant to give the user a chill and nice experience while navigating.",
  "icons": {
    "48": "icons/icon.png"
  },
  "browser_action": {
    "default_icon": {
      "48": "icons/icon.png"
    },
    "default_popup": "popup/popup.html"
  },
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "proxy",
    "storage"
  ],
    
  "background": {
    "scripts": ["./scripts/background/background.js"],
    "persistent": true
  }
}
